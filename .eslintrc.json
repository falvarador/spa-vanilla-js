{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "object-curly-newline": "off",
    "no-throw-literal": "off",
    "quotes": "off",
    "import/extensions": "off",
    "no-unused-vars": "off"
  }
}
